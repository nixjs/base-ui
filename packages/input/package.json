{
    "name": "@nixjs23n6/baseui-input",
    "version": "1.0.15-alpha.0",
    "description": "Input",
    "author": {
        "url": "https://github.com/nixjs",
        "name": "nghinv"
    },
    "homepage": "https://github.com/nixjs/base-ui#readme",
    "license": "MIT",
    "sideEffects": false,
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "browser": "lib/esm/index.js",
    "types": "lib/types/index.d.ts",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "lib"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nixjs/base-ui.git"
    },
    "scripts": {
        "clean": "rimraf lib/*",
        "build:pre": "run-s clean",
        "build:cjs": "npx tsc -p tsconfig.cjs.json",
        "build:esm": "npx tsc -p tsconfig.esm.json",
        "build": "run-s build:pre build:cjs build:esm",
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "format:check": "prettier --check --loglevel error 'src/**/*.{ts,tsx}'",
        "format:write": "prettier --write 'src/**/*.{ts,tsx}'"
    },
    "bugs": {
        "url": "https://github.com/nixjs/base-ui/issues"
    },
    "devDependencies": {
        "@nixjs23n6/baseui-core": "^1.0.15-alpha.0",
        "@nixjs23n6/baseui-hook": "^1.0.15-alpha.0"
    },
    "peerDependencies": {
        "@nixjs23n6/baseui-core": "^1.0.15-alpha.0",
        "@nixjs23n6/baseui-hook": "^1.0.15-alpha.0",
        "classnames": ">=2",
        "react": "^16.8.0 || ^17 || ^18",
        "react-dom": "^16.8.0 || ^17 || ^18",
        "styled-components": ">=5"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.5",
        "react-popper": "^2.3.0"
    }
}
